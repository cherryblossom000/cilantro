{
  "name": "@cherryblossom/cilantro",
  "version": "0.0.1",
  "private": true,
  "publishConfig": {
    "access": "restricted"
  },
  "type": "module",
  "main": "dist/index.js",
  "engines": {
    "node": ">=16.6"
  },
  "scripts": {
    "lint": "eslint --ext ts,cjs .",
    "lint:dev": "dum lint -- --cache",
    "build": "tsc -b",
    "watch": "dum build -- -w",
    "register-commands": "node scripts/dist/register-commands",
    "start": "node .",
    "init-db": "mkdir db",
    "start-db": "mongod --dbpath db",
    "stop-db": "kill $(lsof -ntPi :27017)",
    "deploy": "./scripts/deploy.elv"
  },
  "dependencies": {
    "@discordjs/builders": "0.12.0",
    "@discordjs/opus": "^0.8.0",
    "discord-music-player": "^8.3.1",
    "discord.js": "13.6.0",
    "dotenv": "^16.0.0",
    "mongodb": "^4.3.1",
    "ms": "^2.1.3",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@cherryblossom/eslint-config": "^15.0.0-beta.27",
    "@discordjs/rest": "0.3.0",
    "@types/ms": "^0.7.31",
    "@types/node": "~16.11.22",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "discord-api-types": "0.26.1",
    "eslint": "^8.8.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^37.7.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-unicorn": "^40.1.0",
    "prettier": "^2.5.1",
    "typescript": "~4.5.5"
  }
}
